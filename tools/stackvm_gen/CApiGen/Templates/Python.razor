@using global::CApiGen
@inherits RazorLight.TemplatePage<CommandExtractor>
@{
  DisableEncoding = true;
}

@foreach(var info in Model.OptionInfos) 
{
  if (info.PropertyType.IsEnum)
  {
@:class @(info.PropertyType.Name)(Enum):
    @foreach(var (name, value) in info.PropertyType.RenderEnumFields(LangMode.Pyi))
    {
    @:@(name) = @(value)
    }
  }
}


class @(Model.OptionsType.Name):
    def __init__(self) -> None: ...
@for (int i = 0; i < Model.OptionInfos.Count; i++)
{
    var info = Model.OptionInfos[i];
    @:@(info.PropertyName): @(info.PropertyType.RenderType(LangMode.Pyi))
}
