<rxui:ReactiveUserControl
    x:Class="NnCase.Designer.Modules.Inspector.Inspectors.EnumEditorView"
    x:TypeArguments="local:EnumEditorViewModel"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:rxui="http://reactiveui.net"
    xmlns:local="clr-namespace:NnCase.Designer.Modules.Inspector.Inspectors"
    mc:Ignorable="d" 
    d:DesignHeight="450" d:DesignWidth="800">
    <ComboBox ItemsSource="{Binding Items}" 
		      DisplayMemberPath="Text" SelectedValuePath="Value"
		      SelectedValue="{Binding Value}" Height="23" />
</rxui:ReactiveUserControl>
