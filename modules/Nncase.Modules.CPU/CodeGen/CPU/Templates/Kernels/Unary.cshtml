@model Nncase.CodeGen.CPU.UnaryKernelTemplateModel
@{
    string UnaryToCFunction(UnaryOp op) =>
                op switch
                {
                    UnaryOp.Abs => "mathops::abs",
                    UnaryOp.Acos => "mathops::acos",
                    UnaryOp.Acosh => "mathops::acosh",
                    UnaryOp.Asin => "mathops::asin",
                    UnaryOp.Asinh => "mathops::asinh",
                    UnaryOp.Ceil => "mathops::ceil",
                    UnaryOp.Cos => "mathops::cos",
                    UnaryOp.Cosh => "mathops::cosh",
                    UnaryOp.Exp => "mathops::exp",
                    UnaryOp.Floor => "mathops::floor",
                    UnaryOp.Log => "mathops::log",
                    UnaryOp.Neg => "mathops::neg",
                    UnaryOp.Round => "mathops::round",
                    UnaryOp.Sin => "mathops::sin",
                    UnaryOp.Sinh => "mathops::sinh",
                    UnaryOp.Sqrt => "mathops::sqrt",
                    UnaryOp.Tanh => "mathops::tanh",
                    _ => throw new NotSupportedException($"Unsupported unary: {op}."),
                };
}
unary<@UnaryToCFunction(Model.UnaryOp)>(@(Model.Arguments[0].Symbol.Name).view(), @(Model.Arguments[1].Symbol.Name).view());
